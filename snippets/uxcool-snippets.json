	"popover-custom": {
		"prefix": "ux-popover-custom",
		"body": [
			"<ux-popover :content=\"innerContent\">",
			"${1:自定义标题}",
			"<span slot=\"title\"",
			"style=\"color:red\">标题红</span>",
			"</ux-popover>"
		],
		"description": "悬浮框-自定义标题"
	},

	"Modal": {
		"prefix": "ux-Modal",
		"body": [
			"<ux-modal v-model=\"visible\"",
			"\ttitle=\"${1:Basic}\"",
			"\t@ok=\"onOk($event, 'basic')\"",
			"\t@cancel=\"onCancel($event, 'basic')\"",
			"\t@close=\"onClose\"",
			"\t@after-close=\"${2:onAfterClose}\">",
			"\t<p>contents</p>",
			"\t<p>contents</p>",
			"\t<p>contents</p>",
			"\t<p>contents</p>",
			"</ux-modal>"
		],
		"description": "最基本模态框"
	},

	"Modal-header": {
		"prefix": "ux-Modal-header",
		"body": [
			"<ux-modal v-model=\"headerVisible\"",
			"\t\t@ok=\"onOk($event, 'custom header')\"",
			"\t\t@cancel=\"onCancel($event, 'custom header')\">",
			"\t<div slot=\"title\">",
			"\t\t<ux-icon type=\"Information_o\"></ux-icon> ${1:自定义标题}",
			"\t</div>",
			"\t<p>contents</p>",
			"\t<p>contents</p>",
			"\t<p>contents</p>",
			"\t<p>contents</p>",
			"</ux-modal>"
		],
		"description": "模态框-自定义标题"
	},

	"Modal-footer": {
		"prefix": "ux-Modal-footer",
		"body": [
			"<ux-modal v-model=\"footerVisible\"",
			"\ttitle=\"Custom Footer\"",
			" \t@ok=\"onOk($event, 'custom footer')\"",
			"\t@cancel=\"onCancel($event, 'custom footer')\">",
			"\t<p>contents</p>",
			"\t<p>contents</p>",
			"\t<p>contents</p>",
			"\t<p>contents</p>",
			"\t<div slot=\"footer\">",
			"\t\t<button class=\"ux-btn ux-btn-success\"",
			"\t\t\t@click=\"onSuccess\">success</button>",
			"\t</div>",
			"</ux-modal>"
		],
		"description": "模态框-自定义页脚"
	},

