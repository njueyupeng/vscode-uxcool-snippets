		],
		"description": "datePicker日期控件"
	},

	"dateRangePicker": {
		"prefix": "ux-dateRangePicker",
		"body": [
			"<ux-range-date-picker show-time",
			"\t:placeholder=\"['Start Time', 'End Time']\">",
			"</ux-range-date-picker>"
		],
		"description": "datePicker日期范围控件"
	}


