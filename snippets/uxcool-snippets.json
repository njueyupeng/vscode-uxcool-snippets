	"Select": {
		"prefix": "ux-select",
		"body": [
			"<ux-select style=\"width:200px;\">",
			"\t<ux-option value=\"A\">A</ux-option>",
			"\t<ux-option value=\"B\">B</ux-option>",
			"\t<ux-option value=\"C\">C</ux-option>",
			"</ux-select>"
		],
		"description": "Select下拉框"
	},

	"Select-multiple": {
		"prefix": "ux-select-multiple",
		"body": [
			"<ux-select style=\"width:200px;\" mode=\"multiple\">",
			"\t<ux-option value=\"A\"",
			"\t\tlabel=\"A10\">A10</ux-option>",
			"\t<ux-option value=\"B\"",
			"\t\tlabel=\"B10\">B10</ux-option>",
			"\t<ux-option value=\"C\"",
			"\t\tlabel=\"C10\">C10</ux-option>",
			"</ux-select>"
		],
		"description": "Select下拉框-多选"
	},

	"Select-search": {
		"prefix": "ux-select-search",
		"body": [
			"<ux-select style=\"width:200px;\" show-search>",
			"\t<ux-option value=\"A\">A</ux-option>",
			"\t<ux-option value=\"B\">B</ux-option>",
			"\t<ux-option value=\"C\">C</ux-option>",
			"</ux-select>"
		],
		"description": "Select下拉框-带搜索框"
	},

	"pagination": {
		"prefix": "ux-pagination",
		"body": [
			"<ux-pagination",
			"\tshow-quick-jumper",
			"\t@change=\"changePage\"",
			"\t:current=\"pageInfo.currentPage\"",
			"\t:page-size=\"pageInfo.pageSize\"",
			"\t:show-before-total=\"showBeforeTotal\"",
			"\t:show-after-total=\"(total,pageSize, totalPage, pageNo)=>{ return `共${totalPage}页`}\"",
			"\t:total=\"pageInfo.total\">",
			"</ux-pagination>"
		],
		"description": "pagination分页"
	},

	"tabs": {
		"prefix": "ux-tabs",
		"body": [
			"<ux-tabs>",
			"\t<ux-tab-pane tab=\"Tab 1\"",
			"\t\t\tname=\"1\">",
			"\t\t${1:Tab 1 Content}",
			"\t</ux-tab-pane>",
			"\t<ux-tab-pane tab=\"Tab 2\"",
			"\t\t\tname=\"2\">",
			"\t\t${2:Tab 2 Content}",
			"\t</ux-tab-pane>",
			"\t<ux-tab-pane tab=\"Tab 3\"",
			"\t\t\tname=\"3\">",
			"\t\t${3:Tab 3 Content}",
			"\t</ux-tab-pane>",
			"</ux-tabs>"
		],
		"description": "Tabs标签页"
	},

